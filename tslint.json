{
  "extends": "tslint:latest",
  "rules": {
    "arrow-parens": false,
    "max-line-length": [true, 100],
    "no-console": false,
    "object-literal-key-quotes": [true, "as-needed"],
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "never"]
  }
}
